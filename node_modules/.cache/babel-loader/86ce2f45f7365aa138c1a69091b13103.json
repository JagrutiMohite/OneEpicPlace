\n  },\n  data: function data() {\n    return {\n      users: this.$parent.users\n    };\n  },\n  methods: {\n    updateUsers: function updateUsers() {\n      this.$parent.getUsers();\n    },\n    removeUser: function removeUser(user) {\n      var _this = this;\n\n      var responce = confirm(\"Are you sure you want to delete user - \" + user.username);\n\n      if (responce) {\n        api.admin_RemoveAccount(user.id).then(function (removeResponce) {\n          if (removeResponce == '100') _this.$parent.getUsers();else if (removeResponce == '409') _this.$parent.$refs.Header.logout();\n        });\n      }\n    },\n    openViewUserModal: function openViewUserModal(user) {\n      this.closeModals();\n      this.$refs.ViewUserModal.openModal(user);\n    },\n    openCreateUserModal: function openCreateUserModal() {\n      this.closeModals();\n      this.$refs.CreateUserModal.openModal();\n    },\n    closeModals: function closeModals() {\n      this.$refs.ViewUserModal.closeModal();\n      this.$refs.CreateUserModal.closeModal();\n    }\n  }\n};",{"version":3,"sources":["Users.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAyDA,OAAA,KAAA,GAAA,MAAA,uBAAA,C,CAEA;;AACA,OAAA,aAAA,MAAA,mCAAA;AACA,OAAA,eAAA,MAAA,qCAAA;AAEA,eAAA;AACA,EAAA,UAAA,EAAA;AACA,IAAA,aAAA,EAAA,aADA;AAEA,IAAA,eAAA,EAAA;AAFA,GADA;AAMA,EAAA,IANA,kBAMA;AACA,WAAA;AACA,MAAA,KAAA,EAAA,KAAA,OAAA,CAAA;AADA,KAAA;AAGA,GAVA;AAYA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,OAAA,CAAA,QAAA;AACA,KAHA;AAKA,IAAA,UALA,sBAKA,IALA,EAKA;AAAA;;AACA,UAAA,QAAA,GAAA,OAAA,CAAA,4CAAA,IAAA,CAAA,QAAA,CAAA;;AACA,UAAA,QAAA,EAAA;AACA,QAAA,GAAA,CAAA,mBAAA,CAAA,IAAA,CAAA,EAAA,EAAA,IAAA,CACA,UAAA,cAAA,EAAA;AACA,cAAA,cAAA,IAAA,KAAA,EACA,KAAA,CAAA,OAAA,CAAA,QAAA,GADA,KAEA,IAAA,cAAA,IAAA,KAAA,EACA,KAAA,CAAA,OAAA,CAAA,KAAA,CAAA,MAAA,CAAA,MAAA;AACA,SANA;AAQA;AACA,KAjBA;AAmBA,IAAA,iBAnBA,6BAmBA,IAnBA,EAmBA;AACA,WAAA,WAAA;AACA,WAAA,KAAA,CAAA,aAAA,CAAA,SAAA,CAAA,IAAA;AACA,KAtBA;AAuBA,IAAA,mBAvBA,iCAuBA;AACA,WAAA,WAAA;AACA,WAAA,KAAA,CAAA,eAAA,CAAA,SAAA;AACA,KA1BA;AA2BA,IAAA,WA3BA,yBA2BA;AACA,WAAA,KAAA,CAAA,aAAA,CAAA,UAAA;AACA,WAAA,KAAA,CAAA,eAAA,CAAA,UAAA;AACA;AA9BA;AAZA,CAAA","sourcesContent":["<template>\n    <div class=\"Users\">\n\n        <div class=\"topRow\">\n            <div class=\"usersCol\">Email</div>\n            <div class=\"usersCol\">Username</div>\n            <div class=\"usersCol\">First Name</div>\n            <div class=\"usersCol\">Last Name</div>\n            <div class=\"usersColBtn\">Actions</div>\n        </div>\n\n        <div class=\"users-container\" v-for='(array, index) in this.users' :key=\"'array:'+index\">\n            <div v-if='index === 0 && array.length > 0'      class=\"label\">Admins</div>\n            <div v-else-if='index === 1 && array.length > 0' class=\"label\">Tier 5 Members</div>\n            <div v-else-if='index === 2 && array.length > 0' class=\"label\">Tier 4 Members</div>\n            <div v-else-if='index === 3 && array.length > 0' class=\"label\">Tier 3 Members</div>\n            <div v-else-if='index === 4 && array.length > 0' class=\"label\">Tier 2 Members</div>\n            <div v-else-if='index === 5 && array.length > 0' class=\"label\">Tier 1 Members</div>\n            <div v-else-if='index === 6 && array.length > 0' class=\"label\">Non-Members</div>\n\n            <div class=\"usersRow\" v-for='user in array' :key=\"'user:'+user.id\">\n                <div class=\"usersCol\">\n                    <div class=\"colText\">{{user.email}}</div>\n                </div>\n                <div class=\"usersCol\">\n                    <div class=\"colText\">{{user.username}}</div>\n                </div>\n                <div class=\"usersCol\">\n                    <div class=\"colText\">{{user.firstName}}</div>\n                </div>\n                <div class=\"usersCol\">\n                    <div class=\"colText\">{{user.lastName}}</div>\n                </div>\n                <div class=\"usersColBtn\">\n                    <button class=\"btn btn-primary usersBtn editBtn\" v-on:click=\"openViewUserModal(user)\"></button>\n                    <button class=\"btn btn-dark usersBtn deleteBtn\" v-on:click=\"removeUser(user)\"></button>\n                </div>\n            </div>\n\n            <div class=\"space\" v-if='array.length > 0'></div>\n        </div>\n\n        <button class=\"btn btn-success usersBtn createBtn\" v-on:click=\"openCreateUserModal()\"></button>\n\n        <!-- Modals -->\n        <ViewUserModal ref=\"ViewUserModal\"></ViewUserModal>\n        <CreateUserModal ref=\"CreateUserModal\"></CreateUserModal>\n\n    </div>\n</template>\n\n<style scoped lang=\"scss\">\n    //Personal CSS\n    @import \"./css/Users.css\";\n</style>\n\n<script>\n    import * as api from '@/services/api_access';\n\n    //Modals\n    import ViewUserModal from '../modals/users/ViewUserModal.vue';\n    import CreateUserModal from '../modals/users/CreateUserModal.vue';\n\n    export default {\n        components: {\n            ViewUserModal,\n            CreateUserModal\n        },\n\n        data() {\n            return {\n                users: this.$parent.users\n            }\n        },\n\n        methods: {\n            updateUsers() {\n                this.$parent.getUsers();\n            },\n\n            removeUser(user) {\n                var responce = confirm(\"Are you sure you want to delete user - \" + user.username);\n                if (responce) {\n                    api.admin_RemoveAccount(user.id).then(\n                        removeResponce => {\n                            if (removeResponce == '100')\n                                this.$parent.getUsers();\n                            else if (removeResponce == '409') \n                                this.$parent.$refs.Header.logout();\n                        }\n                    );\n                }\n            },\n\n            openViewUserModal(user) {\n                this.closeModals();\n                this.$refs.ViewUserModal.openModal(user);\n            },\n            openCreateUserModal() {\n                this.closeModals();\n                this.$refs.CreateUserModal.openModal();\n            },\n            closeModals() {\n                this.$refs.ViewUserModal.closeModal()\n                this.$refs.CreateUserModal.closeModal();\n            }\n        }\n    }\n</script>"],"sourceRoot":"src/views/4. admin views/components/views"}]}